  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "preferGlobal": true,
  "bin": {
    "dedupe-csv": "./lib/index.js"
  },
  "engines": {
    "node": ">14.0.0",
